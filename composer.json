{
    "name": "yomeva/open-ai-bundle",
    "description": "This bundles allows you to make calls to the OpenAI APIs.",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["open-ai","openai"],
    "autoload": {
        "psr-4": {
            "Yomeva\\OpenAiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yomeva\\OpenAiBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Arthur Falque Pierrotin",
            "email": "arthur.fp.dev@pm.me",
            "role": "Developer"
        }
    ],
    "require": {
        "symfony/http-kernel": "^7.1",
        "symfony/http-client": "^7.1",
        "symfony/dependency-injection": "^7.1",
        "symfony/config": "^7.1",
        "symfony/validator": "^7.1",
        "symfony/serializer": "^7.1",
        "symfony/expression-language": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/dotenv": "^7.1"
    }
}
